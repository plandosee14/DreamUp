<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://struts.apache.org/dtds/struts-config_1_3.dtd">
<struts-config>
  <action-mappings>
	<action path="/main" forward=".main"/>  
	
	 			
	<action path="/project"
   			         forward=".project"/>
   			         
    <!-- 프로젝트 등록 --> 			         
   	<action path="/menu2"
   			         forward=".menu2"/> 			         
   	<action path="/register_2"
   			         forward=".register2"/>
   	
   	<action path="/register_3"
   			         forward=".register3"/>
   	
   	<action path="/register_4"
   			         forward=".register4"/>
   	
   			         
   	<!-- 로그인/회원가입 -->
   	<action path="/login"
   			         forward=".login"/>
   	<action path="/join"
   			         forward=".join"/>
   	<action path="/joinOK"
   			         forward=".joinOK"/>
   			         
   	<!-- 마이페이지 포워딩 테스트 -->
   	<action path="/mypage" forward=".mypage"></action>         
   			         
  </action-mappings>
</struts-config>